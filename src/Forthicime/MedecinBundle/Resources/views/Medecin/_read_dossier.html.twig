<ul id="recent_read" class="panel nav nav-list" style="font-size: 8pt;">
 	<li class="nav-header"><i class="icon-book icon-white"> </i> Analyses consultées</li>
 	<li>
		<div style="font-weight: bold">Nom</div>
		<div style="font-weight: bold">Consulté le</div>
		<div style="font-weight: bold">Date de l'analyse</div>
	</li>
{% for dossier in latest_read %}
	<li>
		<a href="{{ path('forthicime_dossier', {'id': dossier.id }) }}"> 
			<div>{{ dossier.nom|lower|capitalize  }} {{ dossier.prenom|lower|capitalize }}</div>
			<div>{{ dossier.access|date('d/m/Y H:i')  }} </div>
			<div>{{ dossier.libelle }}</div>	
		</a>
		<span style="clear: both"/>
	</li>						 
{% endfor %}
</ul>